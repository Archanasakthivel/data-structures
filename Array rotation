#include<stdio.h>
#include<stdlib.h>
void main()
{
int a[100],d,n,i,b[100],j,e;
printf("enter the no of elements");
scanf("%d",&n);
printf("enter the no of rotations");
scanf("%d",&d);
printf("enter the array elements");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
for(j=0;j<d;j++)
{
b[j]=a[j];
}
e=n/d;
printf("%d",e);
for(i=0;i<n-d;i++)
{
if(e<=n)
{
a[i]=a[e-1];
e++;
}
}
for(j=0;j<d;j++)
{
a[i]=b[j];
i++;
}
printf("after the rotation ,the elements are:");
for(i=0;i<n;i++)
{
printf("%d\n",a[i]);
}
}
